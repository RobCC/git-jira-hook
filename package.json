{
  "name": "git-jira-hooks",
  "version": "0.1.0",
  "description": "Git hooks with Jira",
  "main": "index.js",
  "bin": {
    "git-jira-hooks": "bin/commit-msg.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobCC/git-jira-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/RobCC/git-jira-hooks/issues"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/RobCC/git-jira-hooks#readme",
  "scripts": {
    "build": "rollup -c",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "bin/pre-commit.js ${HUSKY_GIT_PARAMS}",
      "commit-msg": "bin/commit-msg.js ${HUSKY_GIT_PARAMS} --projectId TEST --config 'dummy/hooks.config.js'"
    }
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "app-root-path": "^3.0.0",
    "lodash.merge": "^4.6.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "rollup": "^2.16.1",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^6.1.0"
  }
}
